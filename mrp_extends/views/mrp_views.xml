<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
  <record id="view_mrp_bom_inherit_form" model="ir.ui.view">
    <field name="name">mrp.bom.inherit.form</field>
    <field name="model">mrp.bom</field>
    <field name="inherit_id" ref="mrp.mrp_bom_form_view"/>
    <field name="arch" type="xml">
        <xpath expr="//field[@name='bom_line_ids']/tree/field[@name='product_qty']" position="replace">
            <field name="quantity_amount"/>
            <field name="decrease_percent"/>
            <field name="product_qty" string="Final Quantity"/>
        </xpath>
    </field>
  </record>
</odoo>